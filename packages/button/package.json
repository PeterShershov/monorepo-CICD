{
  "name": "@petershershov/button",
  "version": "1.0.2",
  "main": "lib/cjs/button.js",
  "module": "lib/esm/button.js",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "run-p prebuild build:cjs build:esm",
    "build:cjs": "babel ./src --presets @babel/preset-env --root-mode upward --copy-files --ignore tests --out-dir lib/cjs",
    "build:esm": "babel ./src --root-mode upward --ignore tests --copy-files --out-dir lib/esm"
  },
  "peerDependencies": {
    "react": "^16.5.2"
  },
  "dependencies": {
    "classnames": "^2.2.6"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "lib"
  ]
}
